package overload;

public class Cat {
	
	public String name;
	public String kind;
	
	public Cat() {
		name = "null";
		kind = "null";
		
	}
	
	public Cat(String n, String k) {
		name = n;
		kind = k;
		System.out.println("지금부터 이 아이의 이름은 " + name + "입니다.");
	}
	
	public void Call() {
		System.out.println("야옹아 이리와");
	}
	
	public void Call(String n) {
		System.out.println(n + "아 이리와");
	}

}
